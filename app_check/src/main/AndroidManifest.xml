<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wms.ui">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wms.ui.AboutActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity
            android:name="com.wms.ui.ScheduleCheckActivity"
            android:label="@string/title_activity_out_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wms.ui.SaleCheckActivity"
            android:label="@string/title_activity_sale"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wms.ui.StorgeCheckActitity"
            android:theme="@style/Base.AppTheme" />
    </application>

</manifest>